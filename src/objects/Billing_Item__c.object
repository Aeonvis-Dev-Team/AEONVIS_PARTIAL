        <errorMessage>Insert order amount in the opportunity in order to calculate percentage</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Percent_Or_Amount_Required</fullName>
        <active>true</active>
        <description>Percent or amount are required</description>
        <errorConditionFormula>ISBLANK(Percent__c) &amp;&amp; ISBLANK(Billing_amount__c)</errorConditionFormula>
        <errorMessage>Percent or amount are required</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Residual_Status</fullName>
        <active>true</active>
        <description>Impedisce che il Residuo cambi stato</description>
        <errorConditionFormula>Residual__c = true

&amp;&amp; 

Not( ISPICKVAL( Billing_status__c , &apos;To be approved&apos;) )</errorConditionFormula>
        <errorDisplayField>Billing_status__c</errorDisplayField>
        <errorMessage>This Billing Item must stay in Status = To be approved</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Status_Invoiced_NumberAndDate_Not_Blank</fullName>
        <active>true</active>
        <description>Per cambiare lo status in &quot;Invoiced&quot; devono esserci il billing number e il billing date</description>
        <errorConditionFormula>ISPICKVAL( Billing_status__c , &apos;Invoiced&apos;) &amp;&amp; (ISBLANK(  Billing_number__c  ) || ISBLANK( Billing_date__c ))</errorConditionFormula>
        <errorMessage>Billing number and date are required to change the status in invoiced</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Status_change_validation</fullName>
        <active>false</active>
        <description>Lo stato del billing item non può mai decrescere e il Not to be invoiced non può mai essere mobificato</description>
        <errorConditionFormula>ISCHANGED(Billing_status__c) &amp;&amp; (
	OR(
		AND (ISPICKVAL(PRIORVALUE(Billing_status__c), &quot;Approved&quot;), ISPICKVAL(Billing_status__c , &quot;To be approved&quot;)),
		AND (ISPICKVAL(PRIORVALUE(Billing_status__c), &quot;Invoice in progress&quot;), OR (ISPICKVAL(Billing_status__c , &quot;To be approved&quot;), ISPICKVAL(Billing_status__c , &quot;Approved&quot;))),
		ISPICKVAL(PRIORVALUE(Billing_status__c), &quot;Invoiced&quot;),
(ISPICKVAL(PRIORVALUE(Billing_status__c), &quot;Not to be invoiced&quot;)
)
)
)</errorConditionFormula>
        <errorDisplayField>Billing_status__c</errorDisplayField>
        <errorMessage>You can&apos;t downgrade billing item status and you can&apos;t change the status of a &quot;Not to be invoiced&quot; item</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Status_notToBeInv_only_from_toBeAppr</fullName>
        <active>false</active>
        <description>Lo stato del billing item può diventare Not to be invoiced solo se lo stato precedente era To be approved</description>
        <errorConditionFormula>AND (
ISCHANGED(Billing_status__c), 
NOT(ISPICKVAL(PRIORVALUE(Billing_status__c), &quot;To be approved&quot;)), ISPICKVAL(Billing_status__c , &quot;Not to be invoiced&quot;)
)</errorConditionFormula>
        <errorDisplayField>Billing_status__c</errorDisplayField>
        <errorMessage>Only item in &quot;To be approved&quot; status can be changed in &quot;Not to be invoiced&quot;</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
</CustomObject>
