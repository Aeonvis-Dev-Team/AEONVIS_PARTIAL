    <validationRules>
        <fullName>Amount_change_onlyif_tobeapproved</fullName>
        <active>true</active>
        <description>L&apos;amount del billing item può essere modificato solo se lo stato è to be approved</description>
        <errorConditionFormula>ISCHANGED( Billing_amount__c) &amp;&amp;
 Residual__c = false &amp;&amp;
OR( 
(NOT(ISCHANGED( Billing_status__c)) &amp;&amp; NOT(ISPICKVAL( Billing_status__c , &apos;To be approved&apos;) )),
ISCHANGED( Billing_status__c) &amp;&amp;  ISPICKVAL( PRIORVALUE(Billing_status__c) , &apos;To be approved&apos;)

)</errorConditionFormula>
        <errorDisplayField>Billing_amount__c</errorDisplayField>
        <errorMessage>You can only change Billing amount of a billing item in status &quot;To be approved&quot;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Approve_validation</fullName>
        <active>false</active>
        <description>Non si può approvare un billing item se è cambiato il relativo amount o se il billing plan è aperto</description>
        <errorConditionFormula>AND (
ISCHANGED( Billing_status__c ),
ISPICKVAL(Billing_status__c , &apos;Approved&apos;),
OR(ISPICKVAL(Opportunity__r.Billin_Plan_Status__c, &apos;Open&apos;), ISBLANK(TEXT(Opportunity__r.Billin_Plan_Status__c))),
ISPICKVAL(PRIORVALUE(Billing_status__c ), &quot;To be approved&quot;)
)</errorConditionFormula>
        <errorDisplayField>Billing_status__c</errorDisplayField>
        <errorMessage>You can&apos;t approve a billing item if the opportunity billing status is open or if you changed the amount of the billing item</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Approve_validation_billto</fullName>
        <active>false</active>
        <description>Non si può approvare un billing item se il bill to è vuoto o non è legato all&apos;account commerciale</description>
        <errorConditionFormula>AND (
ISCHANGED( Billing_status__c ),
ISPICKVAL(Billing_status__c , &apos;Approved&apos;),
OR( ISBLANK( Bill_to__r.Id ),
    AND( Bill_to__r.Id &lt;&gt;  Account__r.Id ,  Bill_to__r.ParentId &lt;&gt; Account__r.Id)),
ISPICKVAL(PRIORVALUE(Billing_status__c ), &quot;To be approved&quot;)
)</errorConditionFormula>
