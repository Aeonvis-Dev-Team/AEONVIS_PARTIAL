        <active>false</active>
        <errorConditionFormula>OR(
  AND(
      Staff_Cost__c &lt;= 0,
      Travel_Cost__c &lt;=0
  ),
   ISBLANK(Staff_Cost__c), 
   ISBLANK(Travel_Cost__c)
)</errorConditionFormula>
