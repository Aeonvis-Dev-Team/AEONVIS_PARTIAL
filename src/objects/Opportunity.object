    <sharingModel>ReadWrite</sharingModel>
    <validationRules>
        <fullName>Billing_plan_status_closed_check</fullName>
        <active>false</active>
        <description>Alla chiusura del billing plan dell&apos;opportunity verifica che il totale degli amount dei billing item siano pari all&apos;amount dell&apos;ordine</description>
        <errorConditionFormula>AND (
ISCHANGED(Billin_Plan_Status__c ),
 ISPICKVAL(Billin_Plan_Status__c , &apos;Closed&apos;),
  Billing_item_total_amount__c &lt;&gt; Order_Amount__c,
   NOT(ISPICKVAL( Engagement_Type__c , &apos;T&amp;M&apos;))  
)</errorConditionFormula>
        <errorDisplayField>Billin_Plan_Status__c</errorDisplayField>
        <errorMessage>The billing plan can&apos;t be closed. The total amount of billing items is not equal to the order amount</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ContractTypeMandatory</fullName>
        <active>false</active>
        <description>The contract type field is mandatory from the proposal phase onwards</description>
        <errorConditionFormula>AND( ISBLANK(TEXT(Contract_type__c)), OR( ISPICKVAL( StageName , &apos;Proposal&apos;), ISPICKVAL( StageName , &apos;Negotiation&apos;), ISPICKVAL( StageName , &apos;Closed Negotiation&apos;), ISPICKVAL( StageName , &apos;Closed Won&apos;)) )</errorConditionFormula>
        <errorDisplayField>Contract_type__c</errorDisplayField>
        <errorMessage>The contract type field is mandatory from the proposal phase onwards</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Contracttype_required_with_order</fullName>
        <active>true</active>
        <description>Contract Type is required when order is filled</description>
        <errorConditionFormula>AND(  NOT(ISBLANK( Order_Number__c )),  ISPICKVAL(Contract_type__c ,&quot;&quot;)  )</errorConditionFormula>
        <errorDisplayField>Contract_type__c</errorDisplayField>
        <errorMessage>Contract Type required when order number is filled</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Controllo_Campi_Opp_Vinta</fullName>
        <active>false</active>
        <description>Controlla il Contractor: se è Aeonvis controlla i campi sull&apos;Account, altrimenti controlla i campi obbligatori su Contractor.</description>
        <errorConditionFormula>(
Partner__r.Name = &apos;Aeonvis&apos;

&amp;&amp;

OR(ISPICKVAL( StageName , &apos;Closed Won&apos;), ISPICKVAL( StageName , &apos;Closed Negotiation&apos;))

&amp;&amp;

OR(Account.VAT_number__c = &apos;&apos;, Account.Company_name__c = &apos;&apos;, Account.BillingCountry = &apos;&apos;, TR_Project_ID__c = &apos;&apos;, ISPICKVAL( Payment_terms__c , &apos;&apos;),  Account.Codice_SDI__c = &apos;&apos;))



||


(
Partner__r.Name != &apos;Aeonvis&apos;

&amp;&amp;

OR(ISPICKVAL( StageName , &apos;Closed Won&apos;), ISPICKVAL( StageName , &apos;Closed Negotiation&apos;))

&amp;&amp;

OR(Partner__r.VAT_number__c = &apos;&apos;, Partner__r.Company_name__c = &apos;&apos;, Partner__r.BillingCountry = &apos;&apos;, TR_Project_ID__c = &apos;&apos;, ISPICKVAL( Payment_terms__c , &apos;&apos;),  Partner__r.Codice_SDI__c = &apos;&apos;))</errorConditionFormula>
        <errorMessage>Compilare: Dati di Fatturazione (nel caso di Contractor diverso da Aeonvis verificare il Contractor), il TR Project ID, i Termini di Pagamento sull&apos;Opportunità.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cost_Required_With_Revenue</fullName>
        <active>true</active>
        <description>Rende obbligatorio il campo costo quando il campo revenue è compilato</description>
        <errorConditionFormula>AND(  NOT(ISBLANK(Amount)),  ISBLANK(Revenue__c) )</errorConditionFormula>
        <errorDisplayField>Revenue__c</errorDisplayField>
        <errorMessage>Cost required when amount is filled</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Elapsed_required_with_order</fullName>
        <active>true</active>
        <description>Elapsed (months) is required when order is filled</description>
        <errorConditionFormula>AND(NOT(ISBLANK( Order_Number__c )),  ISBLANK( Elapsed__c )  )</errorConditionFormula>
        <errorDisplayField>Elapsed__c</errorDisplayField>
        <errorMessage>Elapsed (months) required when order number is filled</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Engagement_type_required_with_order</fullName>
        <active>true</active>
        <description>Engagement Type is required when order is filled</description>
        <errorConditionFormula>AND(  NOT(ISBLANK( Order_Number__c )),  ISPICKVAL( Engagement_Type__c ,&quot;&quot;) )</errorConditionFormula>
        <errorDisplayField>Engagement_Type__c</errorDisplayField>
        <errorMessage>Engagement Type required when order number is filled</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>EngagmentTypeMandatory</fullName>
        <active>false</active>
        <errorConditionFormula>AND( ISBLANK(TEXT( Engagement_Type__c )), OR( ISPICKVAL( StageName , &apos;Proposal&apos;), ISPICKVAL( StageName , &apos;Negotiation&apos;), ISPICKVAL( StageName , &apos;Closed Negotiation&apos;), ISPICKVAL( StageName , &apos;Closed Won&apos;)) )</errorConditionFormula>
        <errorDisplayField>Engagement_Type__c</errorDisplayField>
        <errorMessage>The engagment type field is mandatory from the proposal phase onwards</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>OrderAmount_check_on_insert</fullName>
        <active>false</active>
        <description>All&apos;inserimento dell&apos;order amount, l&apos;order amount deve corrispondere al il totale del billing item (Billing item total amount dell&apos;opportunity)</description>
        <errorConditionFormula>AND(
 ISNULL(PRIORVALUE( Order_Amount__c )),
 ISCHANGED(Order_Amount__c),
 Order_Amount__c &lt;&gt;  Billing_item_total_amount__c 
)</errorConditionFormula>
        <errorDisplayField>Order_Amount__c</errorDisplayField>
        <errorMessage>Billing items total amount is not equal to order amount. Review billing items before insert order amount</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Order_Fullfilled</fullName>
        <active>false</active>
        <description>Verifica che sia stato fatturato tutto e in caso di quality review che questa sia stata completata</description>
        <errorConditionFormula>IF(
AND(Project_closed__c = TRUE, Quality_review__c = TRUE), OR(Billing_item_total_invoiced__c &lt;&gt; Order_Amount__c, !ISPICKVAL(Quality_review_status__c , &quot;Completed&quot;)),
IF(
AND(Project_closed__c = TRUE, Quality_review__c = FALSE),
Billing_item_total_invoiced__c &lt;&gt; Order_Amount__c, FALSE
    )
)</errorConditionFormula>
        <errorMessage>In case of &quot;Order fullfilled&quot; is checked please check &quot;Billing Total Invoiced&quot; = &quot;Amount&quot; and &quot;Quality Status&quot; is completed (only when Quality review is checked)</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Order_Fullfilled_New</fullName>
        <active>true</active>
        <description>Verifica che sia stato fatturato tutto e in caso di quality review che questa sia stata completata</description>
        <errorConditionFormula>IF(
AND(Project_closed__c = TRUE, Quality_review__c = TRUE), OR(Billing_item_total_invoiced__c &lt;&gt; (Order_Amount__c - Billing_item_not_to_be_invoiced__c) , !ISPICKVAL(Quality_review_status__c , &quot;Completed&quot;)),
IF(
AND(Project_closed__c = TRUE, Quality_review__c = FALSE),
Billing_item_total_invoiced__c &lt;&gt; (Order_Amount__c - Billing_item_not_to_be_invoiced__c), FALSE
)
)</errorConditionFormula>
        <errorMessage>In case of &quot;Order fullfilled&quot; is checked please check &quot;Billing Total Invoiced&quot; = &quot;Amount&quot; and &quot;Quality Status&quot; is completed (only when Quality review is checked)</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Order_amount_required</fullName>
        <active>true</active>
        <description>Order amount required when order is filled</description>
        <errorConditionFormula>AND(  NOT(ISBLANK( Order_Number__c )),  ISBLANK(  Order_Amount__c ) )</errorConditionFormula>
        <errorDisplayField>Order_Amount__c</errorDisplayField>
        <errorMessage>Order amount required when order number is filled</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Order_fulfilled_only_for_closed_won</fullName>
        <active>true</active>
        <description>Order fulfilled can be selected only if the opportunity is in Closed won status</description>
        <errorConditionFormula>AND( Project_closed__c  = TRUE, ISBLANK(Order_Number__c))</errorConditionFormula>
        <errorDisplayField>Project_closed__c</errorDisplayField>
        <errorMessage>Order can&apos;t be fulfilled if order number is empty</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Order_required_for_Closed_won_status</fullName>
        <active>true</active>
        <description>Order number is required when an opportunity has status closed won</description>
        <errorConditionFormula>AND(  ISPICKVAL(StageName , &quot;Closed Won&quot;) ,  ISBLANK( Order_Number__c )  )</errorConditionFormula>
        <errorDisplayField>Order_Number__c</errorDisplayField>
        <errorMessage>Order number required for &quot;Closed Won&quot; opportunities</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Orderdate_required_with_order</fullName>
        <active>true</active>
        <description>Order date is required when order is filled</description>
        <errorConditionFormula>AND(NOT(ISBLANK( Order_Number__c )),  ISBLANK( Order_Date__c )  )</errorConditionFormula>
        <errorDisplayField>Order_Date__c</errorDisplayField>
        <errorMessage>Order date required when order number is filled</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Platform_field_required</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
ISBLANK(TEXT(Platform__c )),
NOT(
OR(
ISPICKVAL(StageName, &apos;Closed Won&apos;),
ISPICKVAL(StageName, &apos;Closed Lost&apos;),
ISPICKVAL(StageName, &apos;Budget&apos;)
)
)
)</errorConditionFormula>
        <errorDisplayField>Platform__c</errorDisplayField>
        <errorMessage>Please select a value for the &apos;Platform&apos; field</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Project_close_date_future</fullName>
        <active>true</active>
        <description>Fulfillment date cannot be in the future</description>
        <errorConditionFormula>NOT( ISNULL( Project_closed_date__c ) )&amp;&amp; Project_closed_date__c &gt;  TODAY()</errorConditionFormula>
        <errorDisplayField>Project_closed_date__c</errorDisplayField>
        <errorMessage>Fulfillment date cannot be in the future</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Project_close_date_required</fullName>
        <active>true</active>
        <description>Fulfillment date required when order fulfilled flag is on</description>
        <errorConditionFormula>Project_closed__c  = TRUE  &amp;&amp;  ISNULL(Project_closed_date__c)</errorConditionFormula>
        <errorDisplayField>Project_closed_date__c</errorDisplayField>
        <errorMessage>Fulfillment date is required when the order is fulfilled</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Project_close_required</fullName>
        <active>true</active>
        <description>Order fulfilled flag required when the fulfillment date is filled</description>
        <errorConditionFormula>NOT(ISNULL( Project_closed_date__c ) ) &amp;&amp;  Project_closed__c  = FALSE</errorConditionFormula>
        <errorDisplayField>Project_closed__c</errorDisplayField>
        <errorMessage>Order fulfilled must be selected when you insert a fulfillment date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Quality_manager_mandatory</fullName>
        <active>true</active>
        <description>Quality manager must be filled if flag quality review is active</description>
        <errorConditionFormula>AND( Quality_review__c = TRUE, ISBLANK(  PM_Quality_manager__c  )  )</errorConditionFormula>
        <errorDisplayField>PM_Quality_manager__c</errorDisplayField>
        <errorMessage>Quality manager must be filled if quality review flag is active</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Quality_status_mandatory</fullName>
        <active>false</active>
        <description>Quality review status must be filled if flag quality review is active</description>
        <errorConditionFormula>AND( Quality_review__c = TRUE, ISBLANK(   TEXT(Quality_review_status__c ))  )</errorConditionFormula>
        <errorDisplayField>Quality_review_status__c</errorDisplayField>
        <errorMessage>Quality review status must be filled if quality review flag is active</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Quality_status_mandatory_since_proposal</fullName>
        <active>true</active>
        <description>Quality review status must be filled if opportunity status is equal or greater than proposal</description>
        <errorConditionFormula>AND( Probability &gt;= 0.50, ISBLANK(   TEXT(Quality_review_status__c ))  )</errorConditionFormula>
        <errorDisplayField>Quality_review_status__c</errorDisplayField>
        <errorMessage>Quality review status must be filled if opportunity stage is equal or greater than Proposal</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Start_date_close_date</fullName>
        <active>false</active>
        <description>La data di inizio lavori non può essere inferiore alla data di chiusura opportunità (solo &gt;=)</description>
        <errorConditionFormula>Start_date__c  &lt;  CloseDate</errorConditionFormula>
        <errorDisplayField>Start_date__c</errorDisplayField>
        <errorMessage>The start date must be greater than or equal to the close date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Tr_project_Id_required_with_order</fullName>
        <active>true</active>
        <description>Tr project ID is required when order is filled</description>
        <errorConditionFormula>AND(NOT(ISBLANK( Order_Number__c )),  ISBLANK( TR_Project_ID__c )  )</errorConditionFormula>
        <errorDisplayField>TR_Project_ID__c</errorDisplayField>
        <errorMessage>TR project ID required when order number is filled</errorMessage>
    </validationRules>
